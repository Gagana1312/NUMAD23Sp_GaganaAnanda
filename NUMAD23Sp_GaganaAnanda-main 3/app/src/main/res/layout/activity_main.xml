<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/HelloWorld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        app:layout_constraintBottom_toTopOf="@id/Button5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Button2"
        android:layout_width="143dp"
        android:layout_height="73dp"
        android:text="Clicky Clicky"
        app:layout_constraintBottom_toTopOf="@id/link_collector"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.156"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Button5"
        app:layout_constraintVertical_bias="0.108"
        app:layout_editor_absoluteX="109dp"
        app:layout_editor_absoluteY="277dp" />

    <Button
        android:id="@+id/link_collector"
        android:layout_width="143dp"
        android:layout_height="73dp"
        android:text="Link Collector"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/Button6"
        app:layout_constraintHorizontal_bias="0.123"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Button2"
        app:layout_constraintVertical_bias="0.177"
        app:layout_editor_absoluteX="149dp"
        app:layout_editor_absoluteY="277dp" />

    <Button
        android:id="@+id/Button5"
        android:layout_width="143dp"
        android:layout_height="73dp"
        android:text="About Me"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.332"
        app:layout_editor_absoluteX="149dp"
        app:layout_editor_absoluteY="277dp" />

    <Button
        android:id="@+id/Button6"
        android:layout_width="143dp"
        android:layout_height="73dp"
        android:text="Prime"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.794"
        app:layout_constraintStart_toEndOf="@id/link_collector"
        app:layout_constraintTop_toBottomOf="@id/Button2"
        app:layout_editor_absoluteX="149dp"
        app:layout_editor_absoluteY="277dp" />

    <Button
        android:id="@+id/Location"
        android:layout_width="143dp"
        android:layout_height="73dp"
        android:text="Location"
        app:layout_constraintBottom_toTopOf="@+id/Button6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toEndOf="@+id/Button2"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.8" />


</androidx.constraintlayout.widget.ConstraintLayout>